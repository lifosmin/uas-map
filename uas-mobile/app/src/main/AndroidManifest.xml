<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="id.ac.umn.uas">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UAS"
        tools:targetApi="31">
        <activity android:name=".JobListActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".DetailSeekerActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".SeekerActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".AddJobActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".TrackDetailSeekerActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".TrackSeekerActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".DetailProviderActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".ProviderActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".ChoiceActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity android:name=".SignupActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false">
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>